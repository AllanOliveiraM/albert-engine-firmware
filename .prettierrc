{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "jsxSingleQuote": true,
  "printWidth": 90,
  "arrowParens": "avoid",
  "bracketSpacing": true,
  "plugins": ["@ianvs/prettier-plugin-sort-imports"],
  "importOrder": [
    "<BUILTIN_MODULES>",
    "",
    "^@nestjs/(.*)$",
    "",
    "<THIRD_PARTY_MODULES>",
    "",
    "^[.]",
    "",
    "<TYPES>"
  ],
  "importOrderTypeScriptVersion": "5.7.2",
  "importOrderParserPlugins": ["typescript", "decorators-legacy"]
}
